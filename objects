// Setup
var myObj = {
  gift: "pony",
  pet: "kitten",
  bed: "sleigh"
};

function checkObj(checkProp) {
  // Your Code Here
  switch(checkProp) {
    case "gift":
      return "pony";
    case "pet":
      return "kitten";
    case "bed":
      return "sleigh";
    case "house":
      return "Not Found";
  }
  return "Change Me!";
}

// Test your code by modifying these values
checkObj("gift");
